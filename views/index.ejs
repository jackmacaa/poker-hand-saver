<%- include('partials/header'); -%>
    <div class="home-page">Home</div>
    <a href="/hand-save">Save new hand</a>
    <% for(hand of data) { %>
        <p>
            id:  <%= hand._id  %>
            number of players:  <%= hand.numberOfPlayers  %>
            blinds: sb: <%= hand.sb  %> bb: <%= hand.sb  %>
            date created: <%= hand.dateCreated  %>
            <form action="/<%= hand._id  %>?_method=DELETE" method="POST">
                <input type="submit" value="Delete">
            </form>
        </p>
    <% } %>
<%- include('partials/footer'); -%>
